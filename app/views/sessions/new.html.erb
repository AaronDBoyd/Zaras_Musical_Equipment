<div class="jumbotron">
  <h2>Sign in</h2>
  <%= form_tag signin_path do %>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
      <br>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
      <br>
    <%= submit_tag "Sign In", class:'btn btn-warning' %>
  <% end %>
</div>